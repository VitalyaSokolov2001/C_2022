/******************************************************************************
Напишите перегруженную функцию print(), которая выводит на отдельной строке значения, передаваемые 
ей через запятую. Функция может принимать: 1) int; (2) float; (3) char; (4) int и int; 
(Таким образом, фактически, нужно написать четыре функции)

*******************************************************************************/
#include <iostream>
using namespace std;

void print (int c){

    cout << "c=" << c << endl;
}
void print (float c){

    cout << "c=" << c << endl;
}
void print (char c){

    cout << "c=" << c << endl;
}
void print (int c, int d){

    cout << c << "," << d << endl;
}

int main()
{
  print (20);
  print (1555161633);
  print ('c');
  print (20,45);

return 0;
}
